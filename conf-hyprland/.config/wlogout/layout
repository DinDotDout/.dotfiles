{
    "label" : "lock",
    "action" : "sleep 1; $HOME/.dotfiles/wm_scripts/swaylock.sh",
    "text" : "Lock",
    "keybind" : "l"
}
{
    "label" : "hibernate",
    "action" : "sleep 1; $HOME/.dotfiles/wm_scripts/swaylock.sh && systemctl hibernate",
    "text" : "Hibernate",
    "keybind" : "h"
}
{
    "label" : "logout",
    "action" : "sleep 1; loginctl terminate-user $USER",
    "text" : "Logout",
    "keybind" : "e"
}
{
    "label" : "shutdown",
    "action" : "sleep 1; systemctl poweroff",
    "text" : "Shutdown",
    "keybind" : "s"
}
{
    "label" : "suspend",
    "action" : "sleep 1; $HOME/.dotfiles/wm_scripts/swaylock.sh && systemctl suspend",
    "text" : "Suspend",
    "keybind" : "u"
}
{
    "label" : "reboot",
    "action" : "sleep 1; systemctl reboot",
    "text" : "Reboot",
    "keybind" : "r"
}

